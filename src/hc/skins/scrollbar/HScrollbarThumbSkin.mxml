<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark">
	<fx:Metadata><![CDATA[
    	[HostComponent("spark.components.Button")]
    ]]></fx:Metadata>
	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>
	<s:Rect width="100%" height="14" x="0.5" y="0.5" visible.down="false" visible.disabled="false">
		<s:stroke>
			<s:SolidColorStroke color="#3D5D92" color.over="#C6C6C6" color.up="#C1C1C1" weight="1"/>
		</s:stroke>
		<s:fill>
			<s:SolidColor color="#FFFFFF" color.over="#F9F9F9" color.up="#EAEAEA"/>
		</s:fill>
	</s:Rect>
	<s:Group visible.down="false" rotation="90" horizontalCenter="0" verticalCenter="0" visible.disabled="false">
		<s:Line x="1" xFrom="0" xTo="6" y="0" yFrom="1" yTo="1">
			<s:stroke>
				<s:SolidColorStroke color="#3D5D92" color.over="#C6C6C6" color.up="#C1C1C1" pixelHinting="true" weight="1"/>
			</s:stroke>
		</s:Line>
		<s:Line x="1" xFrom="0" xTo="6" y="0" yFrom="4" yTo="4">
			<s:stroke>
				<s:SolidColorStroke color="#3D5D92" color.over="#C6C6C6" color.up="#C1C1C1" pixelHinting="true" weight="1"/>
			</s:stroke>
		</s:Line>
		<s:Line x="1" xFrom="0" xTo="6" y="0" yFrom="7" yTo="7">
			<s:stroke>
				<s:SolidColorStroke color="#3D5D92" color.over="#C6C6C6" color.up="#C1C1C1" pixelHinting="true" weight="1"/>
			</s:stroke>
		</s:Line>
	</s:Group>
	<s:Rect width="100%" includeIn="disabled,down" height="14" x="0.5" y="0.5" alpha.disabled="0.5">
		<s:stroke>
			<s:SolidColorStroke color="#C1C1C1" weight="1"/>
		</s:stroke>
		<s:fill>
			<s:SolidColor color="#EAEAEA"/>
		</s:fill>
	</s:Rect>
	<s:Group includeIn="disabled,down"  rotation="90" horizontalCenter="0" verticalCenter="0" alpha.disabled="0.5">
		<s:Line x="0.5" xTo="6" y="0.5">
			<s:stroke>
				<s:SolidColorStroke color="#C1C1C1" pixelHinting="true" weight="1"/>
			</s:stroke>
		</s:Line>
		<s:Line x="0.5" xTo="6" y="3.5">
			<s:stroke>
				<s:SolidColorStroke color="#C1C1C1" pixelHinting="true" weight="1"/>
			</s:stroke>
		</s:Line>
		<s:Line x="0.5" xTo="6" y="6.5">
			<s:stroke>
				<s:SolidColorStroke color="#C1C1C1" pixelHinting="true" weight="1"/>
			</s:stroke>
		</s:Line>
	</s:Group>
</s:Skin>
